#!/usr/bin/env python3
"""
MindCore  Sentinel Launcher
"""
import sys
import os

# Add current directory to path so we can import the package locally
sys.path.append(os.path.dirname(os.path.abspath(__file__)))

try:
    from sentinel.cli import main
except ImportError:
    # Fallback if package structure is not ready or path issue
    print("Error: Could not import sentinel package. Run `pip install -e .`", file=sys.stderr)
    sys.exit(1)

if __name__ == "__main__":
    sys.exit(main())
